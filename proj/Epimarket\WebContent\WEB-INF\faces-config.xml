<?xml version="1.0"?>
<!DOCTYPE   faces-config PUBLIC
            "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
            "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

   <application>
    <locale-config>
      <default-locale>fr</default-locale>
      <supported-locale>en</supported-locale>
    </locale-config>
  </application>
    
  <navigation-rule>
    <from-view-id>/*</from-view-id>    
    <navigation-case>
      <from-outcome>transition</from-outcome>
      <to-view-id>/transition.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>market</from-outcome>
      <to-view-id>/market/products.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>visu</from-outcome>
      <to-view-id>/market/visualisation.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>inscription</from-outcome>
      <to-view-id>/inscription.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>caddie</from-outcome>
      <to-view-id>/caddie/caddie.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>admin</from-outcome>
      <to-view-id>/admin/workbench.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>userEdit</from-outcome>
      <to-view-id>/admin/userEdit.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>productEdit</from-outcome>
      <to-view-id>/admin/productEdit.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>addproduct</from-outcome>
      <to-view-id>/admin/addProduct.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>addcategory</from-outcome>
      <to-view-id>/admin/addCategory.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>doCommand</from-outcome>
      <to-view-id>/market/doCommand.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>chartProd</from-outcome>
      <to-view-id>/admin/ProductChart.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>userPage</from-outcome>
      <to-view-id>/membre/userPage.faces</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>historicalPage</from-outcome>
      <to-view-id>/membre/HistoricalPage.faces</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
  
  <managed-bean>
    <description>LangApp</description>
    <managed-bean-name>LangApp</managed-bean-name>
    <managed-bean-class>utils.LangApp</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
     <managed-property>
        <property-name>keyRepository</property-name>
        <value>#{keyRepository}</value>
    </managed-property>
    </managed-bean>

  <managed-bean>
    <description>keyRepository</description>
    <managed-bean-name>keyRepository</managed-bean-name>
    <managed-bean-class>utils.KeyRepository</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  
    
    <!-- 
       BuisnessFacade instance
    -->
    
    
    <managed-bean>
        <description>BuisnessFacade</description>
        <managed-bean-name>buisnessFacade</managed-bean-name>
        <managed-bean-class>ORM.BuisnessFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>


    <!-- 
       Beans instance
    -->

  
